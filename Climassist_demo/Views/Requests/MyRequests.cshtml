@model IEnumerable<Climassist_demo.Models.Requests>

<h2>My Requests</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>User Name</th>
            <th>Email</th>
            <th>Request Type</th>
            <th>Status</th>
            <th>Created At</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var request in Model)
        {
            <tr>
                <td>@request.Id</td>
                <td>@request.UserName</td>
                <td>@request.Email</td>
                <td>@request.RequestType</td>
                <td>@request.Status</td>
                <td>@request.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>
